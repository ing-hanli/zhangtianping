# define  _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
int main()
{
	int num1 = 0;
	int num2 = 0;
	scanf("%d %d", &num1, &num2);
	int sum = num1 + num2;
	printf("%d\n", sum);
	printf("%s\n", " %d");
	printf("\a");//蜂鸣器
	printf("%d\n", 80);
	return 5;
	
}
